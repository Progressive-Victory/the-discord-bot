{
  "name": "the-discord-bot",
  "version": "4.2.0",
  "repository": {
    "type": "git",
    "url": "git+https://https://github.com/Progressive-Victory/the-discord-bot.git"
  },
  "author": {
    "name": "William Bowen",
    "email": "wbowen@progress.win"
  },
  "bugs": {
    "url": "https://github.com/Progressive-Victory/the-discord-bot/issues"
  },
  "license": "MIT",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "pnpm format && tsc --project tsconfig.json",
    "predev": "pnpm build",
    "dev": "node --env-file=.env .",
    "predev-deploy": "pnpm build",
    "dev-deploy": "node --env-file=.env . --deploy",
    "start": "node .",
    "lint": "eslint \"src/**/*.{js,ts,jsx,tsx}\" --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --fix \"src/**/*.{js,ts,jsx,tsx}\" --no-error-on-unmatched-pattern",
    "format": "prettier -w src",
    "doc": "typedoc"
  },
  "engines": {
    "node": ">=22.13.0"
  },
  "dependencies": {
    "@fluent/bundle": "^0.19.1",
    "csv-writer": "^1.6.0",
    "discord.js": "^14.25.1",
    "express": "^5.2.1",
    "mongoose": "^8.20.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.6",
    "@types/node": "^22.19.3",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-tsdoc": "^0.4.0",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1"
  }
}
