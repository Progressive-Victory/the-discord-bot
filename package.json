    "discord.js": "^14.25.1",
    "esbuild": "^0.27.2",
    "express": "^5.2.1",
    "ts-transformer-keys": "^0.4.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.6",
    "@types/node": "^22.19.7",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-tsdoc": "^0.4.0",
